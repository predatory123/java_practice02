package testdemon;

public class Test03Demon {
	public static void main(String[] args) {
			Long zahl = Long.parseLong(args[0]);
			System.out.println(zahl + " 在long的取值范围内：" + ((zahl >= Long.MIN_VALUE &&
					zahl <= Long.MAX_VALUE)));
			System.out.println(zahl + " 在int的取值范围内：" + ((zahl >= Long.MIN_VALUE &&
					zahl <= Long.MAX_VALUE)));
			System.out.println(zahl + " 在short的取值范围内：" + ((zahl >= Long.MIN_VALUE &&
					zahl <= Long.MAX_VALUE)));
			System.out.println(zahl + " 在byte的取值范围内：" + ((zahl >= Long.MIN_VALUE &&
					zahl <= Long.MAX_VALUE)));
	}
}
